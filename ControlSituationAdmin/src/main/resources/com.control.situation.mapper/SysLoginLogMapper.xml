    <sql id="Base_Column_List">
	id,log_name,user_id,create_time,succeed,message,ip
    </sql>

    <select id="findOne" parameterType="java.lang.Integer" resultMap="SysLoginLogResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM sys_login_log
        WHERE id = #{id,jdbcType=BIGINT}
    </select>

    <select id="findAll"  resultMap="SysLoginLogResultMap">
        SELECT
        <include refid="Base_Column_List" />
        FROM sys_login_log
    </select>

    <select id="findByParams" resultMap="SysLoginLogResultMap" parameterType="SysLoginLogInfo">
        SELECT
        <include refid="Base_Column_List" />
        FROM sys_login_log
        <where>
    
	        <if test="id != null" >
			    id = #{id,jdbcType=BIGINT},
	        </if>
    
	        <if test="log_name != null" >
			    log_name = #{logName,jdbcType=VARCHAR},
	        </if>
    
	        <if test="user_id != null" >
