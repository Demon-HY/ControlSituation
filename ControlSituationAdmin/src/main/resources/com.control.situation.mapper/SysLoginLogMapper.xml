        WHERE id = #{id,jdbcType=${tempColumn.columnType}}
    </delete>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.control.situation.sys_login_log.SysLoginLogInfo">
        INSERT INTO
        sys_login_log (log_name,user_id,create_time,succeed,message,ip)
        values
        (#{logName,jdbcType=VARCHAR},#{userId,jdbcType=INTEGER},#{createTime,jdbcType=TIMESTAMP},#{succeed,jdbcType=BIT},#{message,jdbcType=VARCHAR},#{ip,jdbcType=VARCHAR})
    </insert>

    <update id="update" parameterType="com.control.situation.sys_login_log.SysLoginLogInfo">
        UPDATE sys_login_log
        <set>
	    	        	    	        
            <if test="logName != null">
                log_name = #{logName,jdbcType=VARCHAR},
            </if>
	        	    	        
            <if test="userId != null">
                user_id = #{userId,jdbcType=INTEGER},
            </if>
	        	    	        
            <if test="createTime != null">
                create_time = #{createTime,jdbcType=TIMESTAMP},
            </if>
	        	    	        
            <if test="succeed != null">
                succeed = #{succeed,jdbcType=BIT},
            </if>
	        	    	        
            <if test="message != null">
                message = #{message,jdbcType=VARCHAR},
            </if>
	        	    	        
            <if test="ip != null">
                ip = #{ip,jdbcType=VARCHAR},
            </if>
	        	    
