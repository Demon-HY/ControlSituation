<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="orbit_tables" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>

        <property name="endingDelimiter" value="`"/>

        <!-- 不生成注释 -->
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>


        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/control_situation"
                        userId="root" password="123456"/>

        <javaModelGenerator targetPackage="com.control.situation.entity" targetProject="D:\test">
            <!--<property name="rootClass" value=""/>-->
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.control.situation.mapper" targetProject="D:\test" />

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.control.situation.mapper" targetProject="D:\test" />

        <table tableName="sys_dept" domainObjectName="Dept">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_login_log" domainObjectName="LoginLog">
            <generatedKey column="ID" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_menu" domainObjectName="Menu">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_operation_log" domainObjectName="OperationLog">
            <generatedKey column="ID" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_relation" domainObjectName="Relation">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_role" domainObjectName="Role">
            <generatedKey column="ID" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="sys_user" domainObjectName="User">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>

    </context>
</generatorConfiguration>
